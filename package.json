{
  "name": "a10calc",
  "version": "1.0.12",
  "description": "Performance Calculator for DCS A10C",
  "productName": "A10CCalc",
  "author": "cerpogaming <cerpo.gaming@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "start": "quasar dev",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "axios": "^1.6.5",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "html-to-image": "^1.11.11",
    "metar-js": "^0.0.5",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "quasar-extras-svg-icons": "^1.36.0",
    "vue": "^3.4.3",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.3",
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@quasar/app-vite": "^1.7.1",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.18.0",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.3.0",
    "cypress": "^12.17.4",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^28.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.18.1",
    "git-cz": "^4.9.0",
    "prettier": "^3.1.1",
    "semantic-release": "^22.0.12",
    "typescript": "^5.3.3",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
