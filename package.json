  "engines": {
    "node": "^20 || ^21 || ^22",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
