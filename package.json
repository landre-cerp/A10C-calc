    "html-to-image": "^1.11.11",
    "metar-js": "^0.0.6",
    "pinia": "^2.3.0",
    "quasar": "^2.17.7",
    "quasar-extras-svg-icons": "^1.37.0",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "workbox-build": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.5.0",
