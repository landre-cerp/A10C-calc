{
  "name": "a10calc",
  "version": "1.3.16",
  "description": "Performance Calculator for DCS A10C",
  "productName": "A10CCalc",
  "author": "cerpogaming <cerpo.gaming@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "start": "quasar dev -m electron",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "build": "quasar build -m electron",
    "buildmcp": "tsc -p src/mcp/tsconfig.json"
  },
  "resolutions": {
    "typescript": "5.5.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@quasar/cli": "^2.5.0",
    "@quasar/extras": "^1.17.0",
    "@types/ws": "^8.5.11",
    "axios": "^1.12.0",
    "esbuild": "^0.25.11",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "html-to-image": "^1.11.13",
    "metar-js": "^0.0.6",
    "pinia": "^3.0.3",
    "quasar": "^2.18.6",
    "quasar-extras-svg-icons": "^2.2.0",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "vue": "^3.5.20",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.6.3",
    "workbox-build": "^7.0.0",
    "ws": "^8.18.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^20.0.0",
    "@electron/packager": "^19.0.1",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^24.6.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.22",
    "commitizen": "^4.3.1",
    "cypress": "^13.15.2",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^38.2.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.0",
    "git-cz": "^4.9.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "typescript": "^5.9.2",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.3.0"
  },
  "engines": {
    "node": "^20 || ^21 || ^22",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
